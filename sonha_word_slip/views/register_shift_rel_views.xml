<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_register_shift_rel_tree" model="ir.ui.view">
        <field name="name">register.shift.rel.tree</field>
        <field name="model">register.shift.rel</field>
        <field name="arch" type="xml">
            <tree>
                <field name="date"/>
                <field name="shift"/>
            </tree>
        </field>
    </record>

    <record id="register_shift_rel_action" model="ir.actions.act_window">
        <field name="name">Chọn ngày đăng ký ca</field>
        <field name="res_model">register.shift.rel</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
