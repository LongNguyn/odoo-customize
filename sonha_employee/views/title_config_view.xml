<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_config_title_tree" model="ir.ui.view">
        <field name="name">config.title.tree</field>
        <field name="model">title.reward.config</field>
        <field name="arch" type="xml">
            <tree string="Config Title" editable="bottom">
                <field name="id"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="config_title_action" model="ir.actions.act_window">
        <field name="name">Cấu hình Danh hiệu khen thưởng, kỷ luật</field>
        <field name="res_model">title.reward.config</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>