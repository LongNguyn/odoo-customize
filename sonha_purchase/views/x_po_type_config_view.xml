<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sonha_x_po_type_config_tree_view" model="ir.ui.view">
        <field name="name">sonha.x.po.type.config.tree.view</field>
        <field name="model">x.po.type.config</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="x_cancel_validate"/>
                <field name="x_time_approve_po"/>
            </tree>
        </field>
    </record>

    <record id="action_x_po_type_config_view" model="ir.actions.act_window">
        <field name="name">Cài đặt loại PO</field>
        <field name="res_model">x.po.type.config</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>