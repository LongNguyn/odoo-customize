<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sonha_asset_code_tree_view" model="ir.ui.view">
        <field name="name">sonha.asset.code.tree.view</field>
        <field name="model">asset.code</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="descript"/>
<!--                company_id chưa được liên kết với model-->
<!--                <field name="company_id"/>-->
            </tree>
        </field>
    </record>

    <record id="action_asset_code_view" model="ir.actions.act_window">
        <field name="name">Mã tài sản</field>
        <field name="res_model">asset.code</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>